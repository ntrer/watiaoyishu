<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.aliyun.demo.crop" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="26" />

    <application>
        <activity
            android:name="com.aliyun.demo.crop.CropSettingActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AliyunVideoUIStytle"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name="com.aliyun.demo.crop.MediaActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AliyunVideoUIStytle"
            tools:replace="android:theme" >
        </activity>
        <activity
            android:name="com.aliyun.demo.crop.AliyunVideoCropActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AliyunVideoUIStytle"
            tools:replace="android:theme" >
        </activity>
        <activity
            android:name="com.aliyun.demo.crop.AliyunImageCropActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AliyunVideoUIStytle"
            tools:replace="android:theme" >
        </activity>
    </application>

</manifest>