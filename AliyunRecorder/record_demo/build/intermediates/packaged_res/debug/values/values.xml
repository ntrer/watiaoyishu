    <declare-styleable name="BaseScrollPickerView">
        <!-- 中间item的背景-->
        <attr format="reference|color" name="spv_center_item_background"/>
        <!-- 可见的item数量，默认为3个-->
        <attr format="integer" name="spv_visible_item_count"/>
        <!-- 中间item的位置,默认为 mVisibleItemCount / 2-->
        <attr format="integer" name="spv_center_item_position"/>
        <!-- 是否循环滚动，默认为true，开启-->
        <attr format="boolean" name="spv_is_circulation"/>
        <!-- 不允许父组件拦截触摸事件，设置为true为不允许拦截，此时该设置才生效 -->
        <attr format="boolean" name="spv_disallow_intercept_touch"/>
        <!-- 滚动的方向-->
        <attr format="string" name="spv_orientation">
            <enum name="horizontal" value="1"/>
            <enum name="vertical" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="StringScrollPicker">
        <!--文字渐变大小-->
        <attr format="dimension" name="spv_min_text_size"/>
        <attr format="dimension" name="spv_max_text_size"/>
        <!--文字渐变颜色-->
        <attr format="color" name="spv_start_color"/>
        <attr format="color" name="spv_end_color"/>
        <!--文字最大行宽-->
        <attr format="dimension" name="spv_max_line_width"/>
        <!--文字对齐方式-->
        <attr format="enum" name="spv_alignment">
            <enum name="center" value="1"/>
            <enum name="left" value="2"/>
            <enum name="right" value="3"/>
        </attr>
    </declare-styleable>
